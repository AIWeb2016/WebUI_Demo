.ui-tips-guide-arrow-outer-up {
	width: 16px;
	height: 16px;
	position: relative;
	top: -26px;
	left: 0;
	overflow: hidden;
	display: inline-block;
}
.ui-tips-guide-arrow-inner-up {
	width: 10px;
	height: 10px;
	border: 1px solid #ccc;
	box-shadow: 0 0 5px #ccc;
	position: absolute;
	left: 3px;
	top: 10px;
	transform: rotate(45deg);
		-ms-transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	background-color: #fff;
}
.ui-tips-guide-arrow-outer-down {
	width: 16px;
	height: 16px;
	position: absolute;
	top: 31px;
	left: 0px;
	overflow: hidden;
	display: inline-block;
}
.ui-tips-guide-arrow-inner-down {
	width: 10px;
	height: 10px;
	border: 1px solid #ccc;
	box-shadow: 0 0 5px #ccc;
	position: absolute;
	left: 3px;
	top: -6px;
	transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	background-color: #fff;
}
.ui-tips-guide-arrow-outer-left {
	width: 16px;
	height: 16px;
	position: absolute;
	top: 5px;
	left: 0;
	overflow: hidden;
	display: inline-block;
}
.ui-tips-guide-arrow-inner-left {
	width: 12px;
	height: 12px;
	border: 1px solid #ccc;
	box-shadow: 0 0 5px #ccc;
	position: absolute;
	left: 10px;
	top: 1px;
	transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	background-color: #fff;
}
.ui-tips-guide-arrow-outer-right {
	width: 16px;
	height: 16px;
	position: absolute;
	top: 5px;
	left: 10px;
	overflow: hidden;
	display: inline-block;
}
.ui-tips-guide-arrow-inner-right {
	width: 10px;
	height: 10px;
	border: 1px solid #ccc;
	box-shadow: 0 0 5px #ccc;
	position: absolute;
	left: -6px;
	top: 1px;
	transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	background-color: #fff;
}
.ui-tips-guide-content {
	max-width: 250px;
	
	border: 1px solid #ccc;
	border-radius: 5px;
	padding: 10px;
	box-shadow: 0 0 10px #ccc;
	display: inline-block;
	line-height: 21px;
	background-color: #fff;
	font-size: 14px;
}
.ui-tips-guide-icon {
	width: 14px;
	height: 14px;
	margin-right: 5px;
	vertical-align: -2px;
}
.ui-tips-guide-close {
	width: 8px;
	height: 8px;
	margin-left: 5px;
	vertical-align: 2px;
}
.ui-tips-guide-arrow-location{
	position: absolute;
	display: inline-block;
}

