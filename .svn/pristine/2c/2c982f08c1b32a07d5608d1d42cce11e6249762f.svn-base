.ui-flex {
	display: -webkit-box;
	display: -ms-flex;
	display: -webkit-flex;
	display: flex;
}
.ui-flex-hc {
	-webkit-box-pack: center;
	-ms-justify-content: center;
	-webkit-justify-content: center;
	justify-content: center;
}
.ui-flex-vc {
	-webkit-box-align: center;
	-ms-align-items: center;
	-webkit-align-items: center;
	align-items: center;
}
.ui-tips-confirm-overlay,
.ui-tips-alert-overlay {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 1000;
	background-color: rgba(0, 0, 0, 0.5);
}
.ui-tips-toast-overlay {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 1000;
	background-color: rgba(0, 0, 0, 0.0);
}
#guideOverlay {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 1000;
	background-color: rgba(0, 0, 0, 0.0);
}
.ui-tips-confirm-box,
.ui-tips-alert-box,
.ui-tips-confirm-box-no-icon,
.ui-tips-alert-box-no-icon {
	border: 0;
	font-family: "微软雅黑";
	font-size: 15px;
	color: #333;
	background-color: #fff;
	z-index: 1005;
	display: inline-block;
	margin: 0 30px;
	min-width: 240px;
	/*增加圆角*/
	border-radius: 10px;
    overflow: hidden;
}
.ui-tips-toast-box,.ui-tips-toast-box-no-icon {
	/*width: 630px;*/
	/*position: fixed;
	left: 50%;
	top: 50%;
	margin: -130px 0 0 -315px;*/
	top: 100px;
	
	border: 0;
	font-family: "微软雅黑";
	font-size: 15px;
	color: #fff;
	background-color: rgba(0, 0, 0, 0.5);
	z-index: 1005;
	display: inline-block;
	margin-left: 30px;
	margin-right: 30px;
	min-width: 175px;
	text-align: center;
	/*增加圆角*/
	border-radius: 10px;
}
.ui-tips-warning-box {
	width: 100%;
	/*height: 40px;*/
	font-family: "微软雅黑";
	font-size: 14px;
	line-height: 21px;
	text-align: center;
}
.ui-tips-guide-box {
	max-width: 250px;
	/*width:500px;*/
	min-width: 125px;
	height: 40px;
	margin: 0;
	font-family: "微软雅黑";
	font-size: 14px;
	line-height: 21px;
	text-align: center;
}
.normal-yellow {
	color: #333;
	background-color: #fef2bb;
}
.normal-black {
	color: #fff;
	background-color: #333;
}
.emergent-red {
	color: #fff;
	background-color: #f7245b;
}
.ui-tips-confirm-box > h1,
,
.ui-tips-alert-box > h1,
.ui-tips-confirm-box-no-icon > h1,
.ui-tips-alert-box-no-icon > h1,
.ui-tips-confirm-box > p,
.ui-tips-alert-box > p,
.ui-tips-confirm-box-no-icon > p,
.ui-tips-alert-box-no-icon > p,
{
	/*background:url('header_bg.jpg') repeat-x left bottom #f5f5f5;*/
	
	padding: 9px 12px;
	text-shadow: 1px 1px 0 rgba(255, 255, 255, 0.6);
	margin: 0;
	word-wrap:break-word;
	white-space:normal; 
	word-break:break-all;
}
.ui-tips-confirm-box > h1,
.ui-tips-alert-box > h1,
.ui-tips-confirm-box-no-icon > h1,
.ui-tips-alert-box-no-icon > h1 {
	letter-spacing: 0.3px;
	color: #888;
}
.ui-tips-confirm-box-no-icon > p,
.ui-tips-alert-box-no-icon > p {
	background: none;
	line-height: 1.5;
	padding: 26px 20px 0;
	margin: 0;
}
.ui-tips-confirm-box-no-icon > section,
.ui-tips-alert-box-no-icon > section {
	background: none;
	line-height: 20px;
	padding: 10px 20px 26px 20px;
	margin: 0;
	color: #787878;
	font-size: 13px;
	word-wrap:break-word;
	white-space:normal; 
	word-break:break-all;
}
.ui-tips-confirm-box > p,
.ui-tips-alert-box > p {
	background: none;
	line-height: 1.5;
	padding: 13px 20px 0 40px;
	margin: 0;
}
.ui-tips-confirm-box > section,
.ui-tips-alert-box > section {
	background: none;
	line-height: 20px;
	padding: 5px 20px 13px 40px;
	margin: 0;
	color: #787878;
	font-size: 13px;
	word-wrap:break-word;
	white-space:normal; 
	word-break:break-all;
}
.ui-tips-toast-box > p {
	background: none;
	line-height: 1.5;
	padding: 16px 20px 16px 40px;
	margin: 0;
}
.ui-tips-toast-box-no-icon > p {
	background: none;
	line-height: 1.5;
	padding: 16px 20px;
	margin: 0;
}
.ui-tips-warning-box > p {
	padding: 5px 20px;
	margin: 0;
}
.ui-tips-confirm-buttons,
.ui-tips-alert-buttons {
	padding: 0;
	text-align: center;
}
.ui-tips-confirm-title,
.ui-tips-alert-title {
    padding: 0;
    text-align: center;
    font-size: 16px;
    color: #333333;
    line-height: 40px;
    border-bottom: 1px solid #d3d3d3;
}
.ui-tips-confirm-button {
	display: inline-block;
	/*background:url('buttons.png') no-repeat;*/
	
	position: relative;
	height: 40px;
	line-height: 40px;
	width: 50%;
	padding: 0;
	text-decoration: none;
	border-top: 1px;
	border-bottom: 0;
	border-left: 0;
	border-right: 1px;
	border-style: solid;
	border-color: #d3d3d3;
	font-size: 18px;
	z-index: 1015;
}
.ui-tips-alert-button {
	display: inline-block;
	/*background:url('buttons.png') no-repeat;*/
	
	
	position: relative;
	height: 40px;
	line-height: 40px;
	width: 100%;
	padding: 0;
	text-decoration: none;
	border-top: 1px;
	border-bottom: 0;
	border-left: 0;
	border-right: 0px;
	border-style: solid;
	border-color: #d3d3d3;
	font-size: 18px;
}
.ui-tips-button-white{
	color: #44abe5;
	background-color: #FFF;
}
.ui-tips-button-blue{
	color: #fff;
	background-color: #44ABE5;
}
a.ui-tips-button-white:link {} 
a.ui-tips-button-white:visited {}
a.ui-tips-button-white:hover {}
a.ui-tips-button-white:active {
	background-color: #d3d3d3;
	color: #268bc4;
}
a.ui-tips-button-blue:link {} 
a.ui-tips-button-blue:visited {}
a.ui-tips-button-blue:hover {}
a.ui-tips-button-blue:active {
	background-color: #44ABE5;
	color: #bde1fd;
}
.ui-tips-confirm-button:last-child {
	border-right: 0;
	width: 50%;
	margin-left: -1px;
	z-index: 1010;
}
.ui-tips-confirm-button span {
	position: absolute;
	top: 0;
	right: -5px;
	/*background:url('buttons.png') no-repeat;*/
	
	width: 5px;
	height: 33px
}
