.dialog-content {
	overflow: auto;
}
.dialog-toolbar {
	padding: 2px 5px;
}
.dialog-tool-separator {
	float: left;
	height: 24px;
	border-left: 1px solid #ccc;
	border-right: 1px solid #fff;
	margin: 2px 1px;
}
.dialog-button {
	text-align: center;
	height: auto;
	width: 330px;
	padding-bottom: 30px;
	top: 0;
}
.dialog-button .l-btn:first-child {
	margin-right: 5px;
}
.dialog-button .l-btn:last-child {
	margin-left: 5px;
}
.dialog-toolbar,
.dialog-button {
	background: #fff;
}
.dialog-button-2{
	padding-bottom: 20px;
}
