	<pre class="pre_cont"><code class="code_content" style="font-size: 10px;">var dtPikcer=$("#datePicker").AIDtPicker({
	type:'date',
	beginYear:2010,
	endYear:2020,
	buttons:[
		{
			text:'取消',
			action:function(datas){
				console.log(JSON.stringify(datas));
			}
		},
		{
			text:'确定',
			action:function(datas){
				console.log(JSON.stringify(datas));
			}
		}
	]
});

					</code></pre>
