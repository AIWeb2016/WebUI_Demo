			<script>
			$(function(){
				
				$("ul li a").bind("click", function() {
					var _site = $(this).attr("name");
					var _site_a = $(this).children("i").attr("name");
					
					
					$("#iframe_m").attr("src", _site);
					
					if(_site_a == undefined||_site_a == null||_site_a == ""){
						
						$("#iframe_m_api").attr("src", "");
					}
					else if(_site_a != undefined||_site_a != null||_site_a != ""){
							
						$("#iframe_m_api").attr("src", _site_a);
					}
					
				});
			});
				
