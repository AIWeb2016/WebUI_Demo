				<!-- 组件示例区域 -->
				<div class="aries_demo bs-example">
<p class="pp-ps">样式</p>
<hr />
	<select id="com" class="webui-combobox" name="state" disdata-options="editable:false,panelHeight:'auto'" style="width:200px;height: 28px;">
		<option value="AL">业务名称1</option>
		<option value="AL1">业务名称2</option>
		<option value="AL2">业务名称3</option>
		<option value="AL3" disabled="disabled">业务名称4</option>
		<option value="AL4" selected="selected">业务名称5</option>
		<option value="AK">Alaska</option>
		<option value="AZ">Arizona</option>
		<option value="AR">Arkansas</option>
		<option value="CA">California</option>
		<option value="CO">Colorado</option>
		<option value="CT">Connecticut</option>
		<option value="DE">Delaware</option>
		<option value="FL">Florida</option>
		<option value="GA">Georgia</option>
		<option value="HI">Hawaii</option>
		<option value="ID">Idaho</option>
	</select>
	<a href="#nogo" class="webui-linkbutton btn-white" id="btns">获得下拉框值</a>

<script>
	function alert9(mess) {
		$.messager.alert('My Title', mess);
	}
	$("#btns").bind("click", function() {
		var vals = $("#com").combobox("getValue"); //拿val方法
		var text = $("#com").combobox("getText"); //拿val方法
		alert9(text)
	});
</script>
