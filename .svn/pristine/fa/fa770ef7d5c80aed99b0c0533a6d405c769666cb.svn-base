.ui-list {
  padding-left: 0;
  margin-top: 0;
  margin-bottom: 15px;
  list-style: none;
  background-color: #fff;
}
.ui-list .ui-list-item {
  position: relative;
  padding: 18px 20px 18px 15px;
  overflow: hidden;
  border-bottom: 1px solid #ddd;
}
.ui-list .ui-list-item img {
  width: 100px;
}
.ui-list .ui-list-item h4 {
  font-size: 30px;
  font-weight: normal;
  margin: 10px 0;
}
.ui-list .ui-list-item p{
  font-size: 24px;
}
.ui-list .ui-list-item:active {
  color: inherit;
  background-color: #f2f2f2;
}
.ui-list .ui-list-item a {
  position: relative;
  display: block;
  padding: inherit;
  margin: -11px -20px -11px -5px;
  overflow: hidden;
  color: #333;
  font-size: 30px;
  text-decoration: none;
}
.ui-list-item .ui-icon {
  color: #4CB9EC;
  font-size: 46px;
}
.ui-list-item p {
  margin: 0 0 5px 0;
}
.ui-badge {
  display: inline-block;
  padding: 2px 9px 3px;
  font-size: 12px;
  line-height: 1;
  color: #333;
  background-color: rgba(0, 0, 0, 0.15);
  border-radius: 100px;
}
.ui-list-item .ui-badge {
  position: absolute;
  top: 50%;
  right: 15px;
  -webkit-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
}
.ui-list-item .icon-navigate-right .ui-badge {
  right: 35px;
}
.ui-list .ui-media {
  overflow: hidden;
}
.ui-media .fn-left {
  margin-right: 25px;
}
.ui-media .ui-media-body {
  overflow: hidden;
}