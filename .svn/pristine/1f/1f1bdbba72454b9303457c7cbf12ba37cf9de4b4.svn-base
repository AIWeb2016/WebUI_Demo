	<script type="text/javascript">
		$(document).ready(function() {
			$("#tab_group").removeClass().empty();
			$("#tab_group").AITab({
				type: "top",
				field: [{
					name: '控件Demo',
					rel: 'demo',
					icon: ['img/tab_bottom_2.png', 'img/tab_bottom_1.png']
				}, {
					name: '控件API',
					rel: 'api',
					icon: ['img/tab_bottom_2.png', 'img/tab_bottom_1.png']
				}],
				after: function(rel) {
					//alert(rel);
					if (rel == "demo") {
						$("#demo").fadeIn(200);
						$("#api").hide();
					} else if (rel == "api") {
						$("#demo").hide();
						$("#api").fadeIn(200);
					}
				}
			});
		});
	</script>

