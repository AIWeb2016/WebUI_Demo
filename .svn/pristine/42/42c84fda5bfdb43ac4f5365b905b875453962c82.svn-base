div {
	margin: 0;
	padding: 0;
}
/*提示文字配置*/
input,textarea{border:0;-webkit-appearance: none;}
::-webkit-input-placeholder {
	/* WebKit browsers */
	
	font-size: 30px;
	color: #9c9c9c;
}
:-moz-placeholder {
	/* Mozilla Firefox 4 to 18 */
	
	font-size: 30px;
	color: #9c9c9c;
}
::-moz-placeholder {
	/* Mozilla Firefox 19+ */
	
	font-size: 30px;
	color: #9c9c9c;
}
:-ms-input-placeholder {
	/* Internet Explorer 10+ */
	
	font-size: 30px;
	color: #9c9c9c;
}
.ui-textarea::-webkit-input-placeholder {
	/* WebKit browsers */
	
	font-size: 26px;
	color: #9c9c9c;
}
.ui-textarea:-moz-placeholder {
	/* Mozilla Firefox 4 to 18 */
	
	font-size: 26px;
	color: #9c9c9c;
}
.ui-textarea::-moz-placeholder {
	/* Mozilla Firefox 19+ */
	
	font-size: 26px;
	color: #9c9c9c;
}
.ui-textarea:-ms-input-placeholder {
	/* Internet Explorer 10+ */
	
	font-size: 26px;
	color: #9c9c9c;
}
/*提示文字样式配置完成*/
/*输入框*/

.ui-input-div {
	width: 690px;
	height: 90px;
	margin: 0 auto;
	overflow: hidden;
}
.ui-input {
	background-color: #fff;
	width: 658px;
	height: 88px;
	display: inline-block;
	float: left;
	margin: 0 auto;
	font-size: 30px;
	resize: none;
	outline: none;
	padding-left: 30px;
	box-shadow: none;
	border-radius: 0;
	border: 1px solid transparent;
	-webkit-appearance: none;
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
	-webkit-tap-highlight-color: transparent;
}
.ui-input:focus {
	border-color: #44ABE5;
	border-width: 1px;
}
.js-ui-clear {
	display: none;
	position: absolute;
	padding: 15px;
	text-align: center;
	line-height: 50px;
	width: 30px;
	height: 30px;
	margin: 15px 0 0 -70px;
	outline: none;
	resize: none;
	cursor: pointer;
	background: url(../img/clear.png) no-repeat center;
	-webkit-tap-highlight-color: #fff;
}
.ui-input-group {
	width: 690px;
	height: 140px;
	margin: 0 auto;
}
.ui-input-content {
	height: 90px;
	line-height: 90px;
	font-size: 30px;
	background-color: #FFFFFF;
}
.ui-input-content-name {
	text-align: center;
	position: absolute;
	width: 180px;
	height: 90px;
	display: inline;
	float: left;
}
.ui-phone-icon {
	width: 22px;
	height: 30px;
	text-align: center;
	position: absolute;
	display: inline;
	float: left;
	margin-left: 180px;
	margin-top: 30px;
	background: url(../img/mobile.png);
}
.ui-input-val {
	width: 478px;
	padding-left: 212px;
}
.ui-pass-clear {
	margin: 15px 0 0 -125px;
}
.js-ui-look {
	display: inline-block;
	position: absolute;
	padding: 10px;
	width: 44px;
	height: 30px;
	margin: 20px 0 0 -75px;
	-webkit-tap-highlight-color: #fff;
	background: url(../img/look.png) no-repeat center;
}
.ui-input::-ms-clear {
	display: none;
}
.ui-input:valid + .js-ui-clear {
	display: inline;
}
.ui-un-input {
	background-color: #ccc;
	border-color: #9C9C9C;
	color: #9C9C9C;
}
.ui-input-pass {
	border:0;
	-webkit-appearance: none;
}
.ui-input-pass:focus{
	border: 1px solid #44abe5;
}
.ui-input-pass:valid + .ui-look {
	display: inline;
	box-shadow: none;
}
.ui-tip {
	height: 50px;
	width: 100%;
}
.ui-tip p {
	font-size: 26px;
	margin-top: 20px;
	text-indent: 65px;
	color: #F4255B;
}
.ui-tip-error {
	width: 26px;
	height: 26px;
	display: block;
	position: absolute;
	margin-left: 30px;
	background: url(../img/error.png) no-repeat;
}
/*文本域输入*/

.ui-textarea-div {
	width: 690px;
	height: auto;
	margin: 0 auto;
	overflow: hidden;
	border-radius: 0;
}
.ui-textarea{
	width: 688px;
	border: 1px solid transparent;
	line-height: 40px;
	font-size: 26px;
	height: auto;
	resize: none;
	outline: none;
	overflow: hidden;
	border-radius: 0;
}
.ui-textarea-one {
	padding: 23px 30px 23px 30px;
}
.ui-textarea:focus {
	border-color: #44ABE5;
}
.ui-textarea-title-one {
	font-family: "微软雅黑";
	font-size: 30px;
	color: #333;
	margin-bottom: 20px;
}
.ui-textarea-two {
	padding: 83px 30px 23px 30px;
	border: 1px solid transparent;
	z-index: 9;
	position: relative;
	top: -60px;
	left: 0;
}
.ui-textarea-two:focus {
	border-color: #44ABE5;
}
.ui-textarea-title-two {
	width: 686px;
	font-family: "微软雅黑";
	line-height: 30px;
	font-size: 30px;
	text-indent: 30px;
	padding-top: 30px;
	background-color: #fff;
	position: relative;
	top: 1px;
	left: 1px;
	z-index: 10;
}
.ui-textarea-tip {
	width: 100%;
	font-size: 24px;
	color: #787878;
	text-align: right;
	margin-top: 20px;
	display: none;
}
.ui-textarea-row {
	width: 690px;
	height: auto;
	margin: 0 auto;
	overflow: hidden;
}
/*搜索框*/
.ui-search-div {
	width: 690px;
	height: 60px;
	margin: 0 auto;
	overflow: hidden;
	font-family: "微软雅黑";
	border: none;
	font-size: 30px;
	resize: none;
	outline: none;
}
.ui-search {
	width: 628px;
	height: 58px;
	display: block;
	padding-left: 60px;
	border: 1px solid transparent;
	border-radius: 30px;
	-moz-border-radius: 30px;
	-webkit-border-radius: 30px;
}
.ui-search:focus {
	border-color: #44ABE5;
}
.ui-search-icon {
	position: absolute;
	width: 30px;
	height: 30px;
	margin-top: 15px;
	margin-left: 20px;
	background: url(../img/search.png) no-repeat;
}
.ui-search-clear {
	margin: 5px 0 0 -60px;
}
.fn-hide {
	display: none;
}