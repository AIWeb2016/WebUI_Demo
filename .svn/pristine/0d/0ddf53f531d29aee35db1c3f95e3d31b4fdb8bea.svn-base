	font-family: "微软雅黑";
	font-size: 30px;
	display: block;
}
input[type=radio],
input[type=checkbox] {
	display: none;
}
.ui-select-radio-blue-tradition-label:before {
	content: "";
	display: inline-block;
	width: 40px;
	height: 40px;
	float: left;
	background-position-y: 5px;
	background-image: url(../img/radio.png);
	background-size: 30px;
	background-repeat: no-repeat;
}
.ui-select-radio-blue-throughout-label:before {
	content: "";
	display: inline-block;
	width: 40px;
	height: 40px;
	float: right;
	background-position-x: 10px;
	background-position-y: 5px;
	background-image: url(../img/radio.png);
	background-size: 30px;
	background-repeat: no-repeat;
}
.ui-select-checkbox-blue-tradition-label:before {
	content: "";
	display: inline-block;
	width: 40px;
	height: 40px;
	float: left;
	background-position-y: 5px;
	background-image: url(../img/checkbox.png);
	background-size: 30px;
	background-repeat: no-repeat;
}
input[type=radio]:checked + label:before {
	background-image: url(../img/radio-checked.png);
	/*小按钮里面的样式,单选选中状态*/
}
input[type=radio]:disabled + label:before {
	background-image: url(../img/radio-disabled.png);
	/*小按钮里面的样式,单选选中状态*/
}
input[type=checkbox]:checked + label:before {
	background-image: url(../img/checkbox-checked.png);
	/*color: #ffffff;
	background-color: #44ABE5;*/
}
input[type=checkbox]:disabled + label:before {
	background-image: url(../img/checkbox-disabled.png);
}
.ui-select-radio-blue-tradition-ul,
.ui-select-radio-blue-throughout-ul,
.ui-select-checkbox-blue-tradition-ul {
	list-style: none;
	padding: 0;
	margin: 0;
	background-color: #FFFFFF;
}
