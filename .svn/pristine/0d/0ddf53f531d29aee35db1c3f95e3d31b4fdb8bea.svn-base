.ui-select-radio-blue-cycle,
.ui-select-radio-blue-angle,
.ui-select-checkbox-black-angle,
.ui-select-checkbox-blue-cycle,
.ui-select-checkbox-blue-angle {
	display: none;
}
.ui-select-radio-blue-cycle+label,
.ui-select-checkbox-blue-cycle + label {
	background-color: #ffffff;
	border: 2px #44abe5 solid;
	border-radius: 10px;
	display: inline-block;
	position: relative;
	width: 200px;
	height: 66px;
	margin-bottom: 20px;
	margin-left: 20px;
}
.ui-select-radio-blue-cycle + label:after,
.ui-select-checkbox-blue-cycle + label:after {
	content: attr(data-attr);
	position: absolute;
	line-height: 63px;
	width: 100%;
	height: 100%;
	border-radius: 10px;
	color: #44ABE5;
	text-align: center;
	font-family: "微软雅黑";
	font-size: 30px;
}
.ui-select-radio-blue-cycle:checked + label:after,
.ui-select-checkbox-blue-cycle:checked + label:after {
	content: attr(data-attr);
	position: absolute;
	background: #44abe5;
	border: 2px #44abe5 solid;
	color: #FFFFFF;
	margin: -2px 0 0 -2px;
	font-family: "微软雅黑";
	font-size: 30px;
}
