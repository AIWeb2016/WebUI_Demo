* {
    padding: 0;
    margin: 0
}

ul li {
    list-style-type: none
}

a {
    text-decoration: none;
    outline: none
}

body {
    font-family: "Helvetica Neue", Helvetica, Microsoft Yahei, Hiragino Sans GB, WenQuanYi Micro Hei, sans-serif!important;
    font-size: 14px;
    color: #333;
   
}

*:before,
*:after {
    box-sizing: border-box;
}

.clearfix:after {
    clear: both;
}

.clearfix:before,
.clearfix:after {
    display: table;
    content: " ";
}

h1 {
    font-weight: 500;
    font-family: "Helvetica Neue", Helvetica, Microsoft Yahei, Hiragino Sans GB, WenQuanYi Micro Hei, sans-serif;
    line-height: 1.1;
    font-size: 36px;
    border-bottom: 1px solid #eee;
    padding-top: 20px;
    padding-bottom: 9px;
    margin-bottom: 10px
}

p {
    line-height: 1.1
}

h2 {
    font-size: 24px;
    line-height: 1.1;
    margin-top: 20px;
    margin-bottom: 10px;
    font-weight: 500;
    margin: 10px 0
}

.code {
    padding: 2px 4px;
    margin: 0 4px;
    font-size: 12px;
    line-height: 18px;
    color: #c7254e;
    background-color: #f9f2f4;
    border-radius: 4px;
    font-family: Menlo, Monaco, Consolas, "Courier New", monospace;
}

.code_box {
    line-height: 1
}


/*head*/

.header_bar {
    position: fixed;
    width: 100%;
    height: 47px;
    top: 0;
    left: 0;
    background: #424343;
    z-index: 99
}

.header_bar h1 {
    line-height: 3;
    color: #fff;
    padding-left: 30px;
    font-size: 14px;
    font-weight: normal;
}


/*side*/

.wrap_content {
    position: relative;
    overflow: hidden;
    /* height:100%; */
    zoom: 1;
}

.main {
    float: right;
    width: 100%;
    margin-left: -230px;
    margin-top: 47px
}

.main_body {
    margin-left: 230px;
}


