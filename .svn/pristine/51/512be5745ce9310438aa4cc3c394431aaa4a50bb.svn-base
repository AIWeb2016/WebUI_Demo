.cont {
    margin: 0 35px;
}

@media screen and (min-width:1025px) {
    .cont {
        width: 90%;
        margin: 0 auto
    }
}

.view-desc {
    font-size: 16px;
    line-height: 1.75;
    color: #333;
    font-family: "Helvetica Neue", Helvetica, Microsoft Yahei, Hiragino Sans GB, WenQuanYi Micro Hei, sans-serif;
}

pre {
    word-break: break-all;
    word-wrap: break-word;
    color: #333;
    border: none;
    margin: 0;
}

.tab_menu {
    margin-top: 15px;
    overflow: hidden
}

.tab_menu ul li {
    float: left;
    margin: 0 10px;
    background: #ebfaff;
    border: 1px solid #c2daea;
    font-size: 14px;
    font-family: "å¾®è½¯é›…é»‘";
    color: #0857b1;
    height: 30px;
    line-height: 30px;
    padding: 0 35px;
    cursor: pointer
}

.tab_menu ul li:first-child {
    margin-left: 0
}

.tab_menu ul li.tab_menu_current {
    background: #1689da;
    border: 1px solid #116291;
    color: #fff
}

.tab_main .tab_contents {
    display: none;
    margin-top: 10px
}

.tab_contents ul li {
    border-bottom: 1px solid #f3f3f3;
    line-height: 48px;
    padding: 0 20px;
    font-size: 14px;
}

.tab_contents .li_current {
    background: #fbfbfb
}

.tab_contents ul li span {
    display: inline-block
}

.li_open {
    margin: 0 10px
}

.li_open p {
    line-height: 1.8
}

.par {
    margin: 0 20px
}

.par h4 {
    font-weight: 700;
    line-height: 1.4;
    margin: 5px 0
}

.par p {
    padding-left: 24px
}

.tit_line {
    border-bottom: 1px dashed #999;
    line-height: 28px
}

.option_tit {
    position: relative;
    width: 300px;
    padding-left: 14px;
    color: #0397db;
    cursor: pointer
}

.option_tit span.e_required {
    vertical-align: baseline;
    margin-left: 10px
}

.option_def {
    float: right;
    text-align: left;
    width: 200px
}

.option_arrow {
    position: absolute;
    left: 0;
    top: 20px;
    border-width: 4px;
    border-style: dashed dashed dashed solid;
    border-color: transparent transparent transparent #8e8e8e
}

.option_arrow-drop {
    position: absolute;
    left: -1px;
    top: 23px;
    border-width: 4px;
    border-style: solid dashed dashed;
    border-color: #8e8e8e transparent transparent
}

.tab_main h3 {
    font-size: 16px;
    color: #414141;
    padding: 10px 18px;
    font-weight: 500;
    border-bottom: 1px solid #f3f3f3
}

.tab_main h3 span.e_required {
    vertical-align: baseline;
    margin-left: 2px;
    padding-left: 10px
}

.tool_box {
    float: right;
    font-size: 12px;
}

.tool_box a:link,
.tool_box a:visited,
.tool_box a:active {
    color: #0397db;
    text-decoration: none
}

.tool_box a:hover {
    text-decoration: underline
}

.tool_box .split {
    border-right: 1px solid #e2e2e2;
    padding-right: 9px;
    margin-right: 10px
}

.demo_content {
    position: relative;
    margin: 10px 0;
    border: 1px solid #DDD;
    border-radius: 4px;
    background: #FFF;
}

.demo_content:before {
    position: absolute;
    top: 15px;
    left: 15px;
    content: "Example";
    text-transform: uppercase;
    font-size: 12px;
    font-weight: 700;
    color: #959595;
    letter-spacing: 1px;
}

.demo_content:before {
    content: "" !important;
}

.bs-example {
    padding: 20px 15px 10px;
    overflow: hidden;
}

.highlight {
    background: #F7F7F9;
    border-top: 1px solid #ddd;
    position: relative;
}

.highlight pre{
    padding: 10px 15px;
    max-height: 1600px;
}

code, kbd, pre, samp {
    font-family: monaco, menlo, consolas, 'courier new', courier, monospace;
}


/* left side menu */

.side {
    float: left;
    width: 223px;
    background: #fff;
    /* height:100%; */
}

.side_shadow {
    position: fixed;
    width: 223px;
    left: 0;
    top: 0;
    height: 100%;
    box-shadow: 0px 0px 10px 0px rgba(0, 0, 0, 0.2);
}

.demos-nav .components {
    display: none;
}

.demos-nav {
    margin: 47px 0 10px 3px;
    width: 220px;
}

.demos-nav a {
    display: block;
    height: 42px;
    line-height: 42px;
    padding-left: 10px
}

.demos-nav a:link,
.demos-nav a:visited {
    color: #737373;
}

.demos-nav a:hover,
.demos-nav a:active {
    color: #262626;
}

.demos-nav em {
    padding: 0 5px;
}

.demos-nav .category:hover {
    background: #FBFBFB
}

.demos-nav dt {
    position: relative;
    font-size: 13px;
    color: #737373;
    border-bottom: 1px solid #F3F3F3;
    background: #fff;
    cursor: pointer
}

.demos-nav dd a,
.demos-nav li a {
    padding-left: 36px;
    color: #5E5E5E;
    display: block;
    font-size: 13px;
    height: 36px;
    line-height: 36px;
    cursor: pointer;
}

.demos-nav dd a {
    position: relative
}

.demos-nav dd a.new {
    background: url(images/new.png) no-repeat 160px 6px
}

.demos-nav li a.new {
    background: url(images/new.png) no-repeat;
    background-position: 180px 6px
}

.demos-nav .selected {
    background-color: #F1F7FF;
    font-weight: 700;
    color: #262626;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    height: 36px;
    line-height: 36px
}

.demos-nav .selected:before,
.demos-nav .selected:hover:before {
    content: "";
    display: inline-block;
    position: absolute;
    width: 4px;
    height: 36px;
    left: -4px;
    top: 0;
    background: #2AB7D9;
    z-index: 2
}

.demos-nav .plus {
    position: absolute;
    cursor: pointer;
    width: 16px;
    height: 16px;
    top: 10px;
    right: 5px
}

.demos-nav div.components {
    position: relative;
    background: #fbfbfb;
    padding-top: 5px;
    -webkit-box-shadow: inset 0 4px 4px -2px rgba(0, 0, 0, .15), inset 0 -4px 4px -2px rgba(0, 0, 0, .15);
    -moz-box-shadow: inset 0 4px 4px -2px rgba(0, 0, 0, .15), inset 0 -4px 4px -2px rgba(0, 0, 0, .15);
    box-shadow: inset 0 4px 4px -2px rgba(0, 0, 0, .15), inset 0 -4px 4px -2px rgba(0, 0, 0, .15)
}

.demos-nav div.components:before {
    content: "";
    display: block;
    position: absolute;
    z-index: 1;
    left: 23px;
    top: 0px;
    bottom: 15px;
    border-left: 1px solid #E2E2E2;
}

.demos-nav dd ul {
    position: relative;
    display: none;
    border-top: 1px solid #EEE;
    border-bottom: 1px solid #EEE;
    background: #FBFBFB;
    z-index: 46
}

.demos-nav ul li a {
    position: relative;
    padding-left: 60px;
    display: block;
    color: #737373;
    height: 36px;
    line-height: 36px
}

.demos-nav ul li a:hover {
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    color: #262626;
}

.demos-nav dt:hover:before,
.demos-nav ul li dd:hover:before {
    content: "";
    display: inline-block;
    position: absolute;
    width: 4px;
    height: 42px;
    left: -4px;
    top: 0;
    background: #FB6E52;
    z-index: 2;
}

.demos-nav dd {
    position: relative
}

.demos-nav dd:before {
    content: "";
    position: absolute;
    display: inline-block;
    width: 5px;
    height: 4px;
    left: 21px;
    top: 16px;
    background-color: #FFF;
    border: 1px solid #E2E2E2;
    z-index: 2;
}

.demos-nav dd .component:hover:before,
.demos-nav ul li:hover:before {
    content: "";
    display: inline-block;
    position: absolute;
    width: 4px;
    height: 36px;
    left: -4px;
    background: #FB6E52
}

.demos-nav dt .trigger {
    position: absolute;
    right: 5px;
    top: 10px;
    width: 16px;
    height: 16px
}

.demos-nav dt.expand .trigger,
.demos-nav a.open .plus {
    background: url(images/arrow.png) no-repeat 0 -21px
}

.demos-nav dt .trigger,
.demos-nav .component .plus {
    background: url(images/arrow.png) no-repeat 0 0
}

.demos-nav dt.expand a:link,
.demos-nav dt.expand a:visited,
.demos-nav dt.expand a:hover,
.demos-nav dt.expand a:active {
    color: #136CAA;
    font-weight: 700
}

.demos-nav dt.expand:hover {
    background: #fff
}


/*welcome*/

.bd_bg {
    width: 539px;
    height: 530px;
    position: fixed;
    right: 0;
    bottom: 0;
    background: url(images/wel.png) no-repeat right bottom
}

.wel_box {
    padding: 20px 30px;
    color: #737373
}

.wel_box h1 {
    border: 0
}

.wel_box ul li {
    line-height: 2.0;
    font-size: 14px
}

.btn-clipboard {
    background-color: #FFFFFF;
    border-bottom: 1px solid #E1E1E8;
    border-left: 1px solid #E1E1E8;
    border-radius: 0 4px 0 4px;
    color: #767676;
    cursor: pointer;
    display: block;
    font-size: 12px;
    padding: 5px 8px;
    position: absolute;
    right: 0;
    top: 0;
    z-index: 10;
}

.btn-clipboard-hover {
    background-color: #563D7C;
    border-color: #563D7C;
    color: #FFFFFF;
}


/**
 * Baby Blue theme for RainbowJS
 *
 * @author tilomitra
 */

pre .comment {
    color: #999;
}

pre .tag,
pre .tag-name,
pre .support.tag-name {
    color: rgb(85, 85, 85);
}

pre .keyword,
pre .css-property,
pre .vendor-prefix,
pre .sass,
pre .class,
pre .id,
pre .css-value,
pre .entity.function,
pre .storage.function {
    font-weight: bold;
}

pre .css-property,
pre .css-value,
pre .vendor-prefix,
pre .support.namespace {
    color: #333;
}

pre .constant.numeric,
pre .keyword.unit,
pre .hex-color {
    font-weight: normal;
    color: #099;
}

pre .attribute,
pre .variable,
pre .support {
    color:  #757575; /* skinbuilder block-page-text-normal with #1f8dd6 as primary */
}

pre .string,
pre .support.value  {
    font-weight: normal;
    color: #3b8bba; /* skinbuilder block-mine-text-low with #1f8dd6 as primary */
}